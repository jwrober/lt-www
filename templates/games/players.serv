set aifill {{ joineds | length }}
# Make sure they are humans
{% for p in joineds %}aitoggle AI*{{ forloop.counter }}
{% endfor %}
# Set nations
{% for p in joineds %}playernation AI*{{ forloop.counter }} {{ p.nation }} 1 {{ p.user }}
{% endfor %}
{% if game.mode == 'team game' %}
# Set teams
team {{ p.user }} "{{ p.team }}"
{% endif %}
